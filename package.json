{
  "name": "cmd-forum",
  "version": "1.2.0",
  "private": true,
  "scripts": {
    "dev": "next dev --experimental-https --turbo",
    "build": "next build",
    "build:docker": "docker build --platform linux/amd64 -t cmd-forum .",
    "docker:startup": "next start && npx prisma migrate dev",
    "start": "next start",
    "lint": "next lint",
    "migrate": "dotenv -e .env -- npx prisma migrate dev --name init",
    "migrate:dev": "dotenv -e .env.local -- npx prisma migrate dev --name init",
    "dbpush": "dotenv -e .env -- npx prisma db push",
    "dbpush:dev": "dotenv -e .env.local -- npx prisma db push",
    "studio": "dotenv -e .env -- npx prisma studio",
    "studio:dev": "dotenv -e .env.local -- npx prisma studio",
    "postinstall": "npx prisma generate",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "setup": "node setup.js"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.3.3",
    "@floating-ui/react": "^0.26.16",
    "@heroicons/react": "^2.1.3",
    "@hookform/resolvers": "^3.3.4",
    "@next/eslint-plugin-next": "^14.2.3",
    "@prisma/client": "^5.15.0",
    "@simplewebauthn/browser": "^9.0.1",
    "@simplewebauthn/server": "^9.0.3",
    "@uiw/react-markdown-editor": "^6.0.0",
    "@uiw/react-markdown-preview": "^5.1.1",
    "@vercel/speed-insights": "^1.0.10",
    "bcryptjs": "^2.4.3",
    "conventional-changelog-cli": "^5.0.0",
    "dotenv-cli": "^7.4.1",
    "framer-motion": "^11.1.7",
    "next": "^14.2.3",
    "next-auth": "^5.0.0-beta.17",
    "next-dev-https": "^0.13.3",
    "nextjs-toploader": "^1.6.12",
    "prisma-docs-generator": "^0.8.0",
    "prompts": "^2.4.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.3",
    "react-icons": "^5.0.1",
    "rehype-sanitize": "^6.0.0",
    "sass": "^1.69.5",
    "use-debounce": "^10.0.0",
    "xss": "^1.0.15",
    "zod": "^3.23.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20.10.6",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/parser": "^7.12.0",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "^14.1.0",
    "eslint-plugin-deprecation": "^3.0.0",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "postcss": "^8",
    "prisma": "^5.15.0",
    "tailwindcss": "^3.4.0",
    "tailwindcss-themer": "^4.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  }
}
